<ul class="metrics">
  <% metrics.sort_by { |id, metric| metric.name }.each do |id, metric| %>
    <li class="metric" id="metric_<%= id %>">
      <%= render Vanity.template("metric"), :id=>id, :metric=>metric %>
    </li>
  <% end %>
</ul>
<form id="milestones">
  <table border="0" cellspacing="5" cellpadding="5">    
    <% experiments.each do |id, experiment| %>
      <tr>
        <td>
          <input type="checkbox" name="milestone" data-start="<%= experiment.created_at.httpdate %>" data-end="<%= (experiment.completed_at || Time.now).httpdate %>">
        </td>
        <td>
          <label><%=h experiment.name %></label>
        </td>
      </tr>
    <% end %>  
  </table>  
</form>
